                                <div class="innercol--properties">
                                    <div class="listing-properties">
                                        <div class="listing-properties-img"><img src="/images/listingtype.png" alt="thumbnail"></div>
                                        <div class="listing-properties-title"><%= listing.type %></div>
                                    </div>
                                    <div class="listing-properties">
